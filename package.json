{
  "name": "superconnectx",
  "version": "1.0.3",
  "description": "SuperConnectX - 超级连接",
  "main": "./out/main/index.js",
  "author": "SuperStudio",
  "homepage": "https://github.com/SuperStudio/SuperConnectX",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@element-plus/icons-vue": "^2.3.2",
    "electron-store": "^3.3.0",
    "element-plus": "^2.11.9",
    "monaco-editor": "^0.55.1",
    "ssh2": "^1.17.0",
    "telnet-client": "^2.2.9",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@types/electron-store": "^1.3.1",
    "@types/node": "^22.18.6",
    "@types/ssh2": "^1.15.5",
    "@types/winston": "^2.4.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^38.1.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.1",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vue": "^3.5.21",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  },
  "build": {
    "appId": "superconnectx.superstudio",
    "productName": "superconnectx",
    "copyright": "Copyright © 2025 SuperStudio",
    "directories": {
      "output": "release"
    },
    "files": [
      "out/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns",
      "hardenedRuntime": true
    },
    "linux": {
      "target": [
        "deb",
        "rpm"
      ],
      "icon": "build/icon.png"
    }
  }
}
